txt << 'id'
txt << 'created_at'
txt << 'read_at'
@form.fields.each do |field|
  txt << field.title
end
txt.newline

@messages.each do |msg|
  txt << msg.id.to_s # Записываем идентификатор сообщения
  txt << msg.created_at.to_s
  txt << msg.read_at.to_s
  msg.data.each do |f|
    txt << f.to_s
  end
  txt.newline
end